<model 
	xmlns:i18n="http://xml.zope.org/namespaces/i18n"
	xmlns:marshal="http://namespaces.plone.org/supermodel/marshal"
	xmlns:form="http://namespaces.plone.org/supermodel/form"
	xmlns:security="http://namespaces.plone.org/supermodel/security"
	xmlns:users="http://namespaces.plone.org/supermodel/users"
	xmlns:lingua="http://namespaces.plone.org/supermodel/lingua"
	xmlns="http://namespaces.plone.org/supermodel/schema">
  <schema>
    <field name="label" type="zope.schema.TextLine">
      <description/>
      <required>False</required>
      <title>Label</title>
    </field>
    <field name="growthphase" type="zope.schema.Choice">
      <description/>
      <required>False</required>
      <title>GrowthPhase</title>
      <values>
        <element>Immature</element>
        <element>Vegetative</element>
        <element>Flowering</element>
        <element>Harvest</element>
      </values>
    </field>
  </schema>
</model>
